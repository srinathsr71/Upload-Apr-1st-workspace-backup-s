package com.pr;

import java.util.Scanner;

public class progra {
    public static void main(String[] args) {
        int[] arr = {0, 0, 1, 1, 2, 0, 0, 2, 1, 0, 1, 2};
        

        int count = 0, count1 = 0, count2 = 0;

        // Count the occurrences of each number
        for (int i = 0; i < arr.length; i++) {
            if (arr[i] == 0) {
                count++;
            } else if (arr[i] == 1) {
                count1++;
            } else if (arr[i] == 2) {
                count2++;
            }
        }

        // Print groups
        for (int i = 0; i < count; i++) {
            System.out.print("0");
        }
        System.out.print(" ");

        for (int i = 0; i < count2; i++) {
            System.out.print("2");
        }
        System.out.print(" ");

        for (int i = 0; i < count1; i++) {
            System.out.print("1");
        }
    }
}



